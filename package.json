{
  "name": "11ty-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf _site",
    "dev:styles:compile": "sass --watch --source-map --embed-sources styles/styles.scss:_site/styles/temp.css",
    "dev:styles:process": "postcss -w _site/styles/temp.css -o _site/styles/styles.css",
    "dev:styles": "run-p dev:styles:*",
    "dev:scripts": "rollup -cwm inline",
    "dev:11ty": "ELEVENTY_ENV=development npx @11ty/eleventy --serve",
    "predev": "npm run clean",
    "dev": "run-p dev:11ty dev:styles dev:scripts",
    "build:styles:compile": "sass --no-source-map styles/styles.scss:_site/styles/temp.css",
    "build:styles:process": "postcss _site/styles/temp.css -o _site/styles/styles.css",
    "build:styles": "run-s build:styles:compile build:styles:process",
    "build:scripts": "rollup -c",
    "postbuild:scripts": "npx babel _site/scripts/bundled --out-dir _site/scripts/legacy",
    "build:11ty": "ELEVENTY_ENV=production npx @11ty/eleventy",
    "prebuild": "npm run clean",
    "build": "run-s build:11ty build:styles build:scripts",
    "postbuild": "html-minifier --collapse-whitespace --remove-comments --remove-optional-tags --remove-redundant-attributes --remove-script-type-attributes --remove-tag-whitespace --use-short-doctype --minify-css true --minify-js true --input-dir _site --output-dir _site --file-ext html"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^0.9.0",
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "babel-preset-minify": "^0.5.1",
    "cssnano": "^4.1.10",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.21",
    "postcss-cli": "^6.1.3",
    "postcss-preset-env": "^6.7.0",
    "rollup": "^1.27.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.1.3",
    "sass": "^1.23.3",
    "terser": "^4.4.0"
  }
}
